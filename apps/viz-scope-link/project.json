{
  "name": "viz-scope-link",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/viz-scope-link/src",
  "projectType": "application",
  "tags": ["type:app"],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite",
        "cwd": "apps/viz-scope-link"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "vite build",
        "cwd": "apps/viz-scope-link"
      },
      "configurations": {
        "development": {
          "command": "vite build --mode development",
          "cwd": "apps/viz-scope-link"
        }
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite preview",
        "cwd": "apps/viz-scope-link"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint .",
        "cwd": "apps/viz-scope-link"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run",
        "cwd": "apps/viz-scope-link"
      }
    },
    "test:watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest",
        "cwd": "apps/viz-scope-link"
      }
    }
  }
}
